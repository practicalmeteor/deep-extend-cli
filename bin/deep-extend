#!/usr/bin/env node

var deepExtend = require('deep-extend');

if (process.argv.length != 4) {
	console.error("Error: Please provide exactly 2 json files as command line arguments.");
	process.exit(1);
}

var path = require('path');

// First 2 argvs are node and this file
var obj1 = require(path.resolve(process.argv[2]));
var obj2 = require(path.resolve(process.argv[3]));

deepExtend(obj1, obj2);

console.log(JSON.stringify(obj1, null, 4));
